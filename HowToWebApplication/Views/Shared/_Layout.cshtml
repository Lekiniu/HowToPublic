<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Panel Demo</title>
    <link rel="icon" type="image/ico" href="~/images/atom-icon.svg" />

    @*<title>@ViewBag.Title - My ASP.NET Application</title>
        @Styles.Render("~/Content/css")
        @Scripts.Render("~/bundles/modernizr")*@

    <link href="~/Content/bootstrap.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <link href="~/Content/themes/base/jquery-ui.min.css" rel="stylesheet" />


    <!-- layoyt css files -->
    <link href="~/Content/AdminPanelStyles/core.css" rel="stylesheet" />
    <link href="~/Content/AdminPanelStyles/components.css" rel="stylesheet" />
    <link href="~/Content/AdminPanelStyles/icons.css" rel="stylesheet" />
    <link href="~/Content/AdminPanelStyles/pages.css" rel="stylesheet" />
    <link href="~/Content/AdminPanelStyles/responsive.css" rel="stylesheet" />

    <script src="~/Scripts/AdminPanelScripts/wow.min.js"></script>

    @*<link href="~/Content/AdminPanelStyles/animsition.min.css" rel="stylesheet" />
        <link href="~/Content/AdminPanelStyles/main.css" rel="stylesheet" />*@

    <!-- js files -->
    <script src="~/Scripts/jquery-3.3.1.js"></script>
    <script src="~/Scripts/jquery-ui-1.12.1.js"></script>
    <script src="~/Scripts/jquery.unobtrusive-ajax.min.js"></script>

    @*<script async defer src="https://buttons.github.io/buttons.js"></script>*@

    <!-- datatables css -->
    <link href="~/Scripts/datatableLib/DataTables-1.10.18/css/dataTables.semanticui.min.css" rel="stylesheet" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/semantic-ui/2.3.1/semantic.min.css" rel="stylesheet" />
    <link href="~/Content/_popUpSharedStyle.css" rel="stylesheet" />

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.css">
    <style>
        .tz-gallery img {
            height: 100%;
        }

        .tz-gallery a {
            width: 150px;
            height: 150px;
            display: inline-block;
            overflow: hidden;
            margin: 4px 6px;
            box-shadow: 0 0 4px -1px #000;
        }
    </style>

    <style>
        body {
            background: white;
            height: auto;
        }

        .footer {
            background: #ebeff2;
            bottom: auto;
        }



        .media-body h5 {
            line-height: 35px;
        }

     
        .disabled {
            pointer-events: none;
            cursor: default;
        }
    </style>

</head>

<body class="fixed-left">

    <!-- Begin page -->
    <div id="wrapper">

        <!-- Top Bar Start -->
        <div class="topbar">

            <!-- LOGO -->
            <div class="topbar-left topbar-left-sm">
                <div class="text-center">
                    <a href="@Url.Action("Index", "Admin")" class="logo"><span>How <img src="~/images/atom-icon.svg" height="20"/> To</span></a>
                    <!-- Image Logo here -->
                    <!--<a href="index.html" class="logo">-->
                    <!--<i class="icon-c-logo"> <img src="assets/images/logo_sm.png" height="42"/> </i>-->
                    @*<span><img src="~/images/atom-icon.svg" height="20"/></span>*@
                    <!--</a>-->
                </div>
            </div>

            <!-- Button mobile view to collapse sidebar menu -->
            <div class="navbar navbar-default" role="navigation">
                <div class="container">
                    <div class="">
                        <div class="pull-left">
                            <button class="button-menu-mobile open-left waves-effect waves-light">
                                <i class="md md-menu"></i>
                            </button>
                            <span class="clearfix"></span>
                        </div>



                        <form role="search" class="navbar-left app-search pull-left hidden-xs">
                            <input type="text" placeholder="Search..." class="form-control">
                            <a href=""><i class="fa fa-search"></i></a>
                        </form>


                        <ul class="nav navbar-nav navbar-right pull-right">
                            <li>
                                @Html.Partial("/Views/Shared/_LanguagesShareView.cshtml")
                            </li>
                            <li class="dropdown top-menu-item-xs">
                                <a href="#" @*data-target="#"*@ class="dropdown-toggle waves-effect waves-light" data-toggle="dropdown" aria-expanded="true">
                                    <i class="icon-plus"></i>@*<span class="badge badge-xs badge-danger"></span>*@
                                </a>
                                <ul class="dropdown-menu dropdown-menu-lg">
                                    <li class="notifi-title">@*<span class="label label-default pull-right">New 3</span>*@Actions</li>
                                    <li class="list-group slimscroll-noti notification-list">
                                        <!-- list item-->
                                        <a href="@Url.Action("CreateArticles", "Admin")" data-toggle="modal" class="list-group-item Create-form">
                                            <div class="media">
                                                <div class="pull-left p-r-10">
                                                    <em class="fa fa-diamond noti-primary"></em>
                                                </div>
                                                <div class="media-body">
                                                    <h5 class="media-heading">Add New Article</h5>

                                                </div>
                                            </div>
                                        </a>

                                        <!-- list item-->
                                        <a href="javascript:void(0);" class="list-group-item">
                                            <div class="media">
                                                <div class="pull-left p-r-10">
                                                    <em class="fa fa-cog noti-warning"></em>
                                                </div>
                                                <div class="media-body">
                                                    <h5 class="media-heading">Add New Category</h5>

                                                </div>
                                            </div>
                                        </a>

                                        <!-- list item-->
                                        <a href="javascript:void(0);" class="list-group-item">
                                            <div class="media">
                                                <div class="pull-left p-r-10">
                                                    <em class="fa fa-bell-o noti-custom"></em>
                                                </div>
                                                <div class="media-body">
                                                    <h5 class="media-heading">Add New Request</h5>

                                                </div>
                                            </div>
                                        </a>

                                        <!-- list item-->
                                        <a href="javascript:void(0);" class="list-group-item">
                                            <div class="media">
                                                <div class="pull-left p-r-10">
                                                    <em class="fa fa-user-plus noti-pink"></em>
                                                </div>
                                                <div class="media-body">
                                                    <h5 class="media-heading">Add New User</h5>

                                                </div>
                                            </div>
                                        </a>




                                </ul>
                            </li>




                            <li class="hidden-xs">
                                <a href="#" id="btn-fullscreen" class="waves-effect waves-light"><i class="icon-size-fullscreen"></i></a>
                            </li>


                            @Html.Partial("_LoginPartial")
                        </ul>
                    </div>
                    <!--/.nav-collapse -->
                </div>
            </div>
        </div>
        <!-- Top Bar End -->
        <!-- ========== Left Sidebar Start ========== -->

        <div class="left side-menu side-menu-sm">
            <div class="sidebar-inner slimscrollleft">
                <!--- Divider -->
                <div id="sidebar-menu">
                    <ul>
                        <li class="has_sub">
                            <a href="@Url.Action("Index", "Admin")" class="waves-effect"><i class="ti-home"></i> <span> @Resources.Global.Dashboard </span> </a>
                        </li>
                        <li class="has_sub">
                            <a href="@Url.Action("ArticlesList", "Admin")" class="waves-effect"><i class="ti-files"></i> <span> @Resources.Global.Articles </span> </a>
                        </li>
                        <li class="has_sub">
                            <a href="@Url.Action("CategoriesList", "Admin")"  class=" disabled waves-effect"><i class="ti-view-grid"></i> <span> @Resources.Global.Categories </span> </a>
                        </li>
                        <li class="has_sub">
                            <a href="@Url.Action("RequestsList", "Admin")"  class="disabled waves-effect"><i class="ti-help-alt"></i> <span> @Resources.Global.Requests </span> </a>
                        </li>
                        <li class="has_sub">
                            <a href="@Url.Action("UsersList", "Admin")"  class="disabled waves-effect"><i class="ti-user"></i> <span> @Resources.Global.Users </span> </a>
                        </li>
                        <li class="has_sub">
                            <a href="@Url.Action("ContactList", "Admin")"  class="disabled waves-effect"><i class="ti-email"></i> <span> @Resources.Global.Contact </span> </a>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div class="clearfix"></div>
            </div>
        </div>
        <!-- Left Sidebar End -->
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="content-page">
            <!-- Start content -->
            <div class="content">
                <div class="container">
                    @RenderBody()
                    @*<div style="min-height: 3000px;"></div>*@
                </div> <!-- container -->
            </div> <!-- content -->
           
            <footer class="footer text-right">
                © 2016. All rights reserved.
            </footer>

        </div>

        <!-- ============================================================== -->
        <!-- End Right content here -->
        <!-- ============================================================== -->
        <!-- modal placeholder-->
        <div id='myAccountModal' class='modal fade in'>
            <div class="modal-dialog">
                <div class="modal-content">
                    <div id='myAccountModalContent'></div>
                </div>
            </div>
        </div>

        <div id='myLoginModal' class='modal fade in'>
            <div class="modal-dialog">
                <div class="modal-content">
                    <div id='myLoginModalContent'></div>
                </div>
            </div>
        </div>

    </div>

    @*@Scripts.Render("~/bundles/jquery")
        @Scripts.Render("~/bundles/bootstrap")*@

    <script>
        var resizefunc = [];
    </script>

    <!-- js files -->
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>

    <!-- layoyt js files -->
    <script src="~/Scripts/AdminPanelScripts/detect.js"></script>
    <script src="~/Scripts/AdminPanelScripts/fastclick.js"></script>
    <script src="~/Scripts/AdminPanelScripts/jquery.slimscroll.js"></script>
    <script src="~/Scripts/AdminPanelScripts/jquery.blockUI.js"></script>
    <script src="~/Scripts/AdminPanelScripts/waves.js"></script>
    <script src="~/Scripts/AdminPanelScripts/wow.min.js"></script>
    <script src="~/Scripts/AdminPanelScripts/jquery.nicescroll.js"></script>
    <script src="~/Scripts/AdminPanelScripts/jquery.scrollTo.min.js"></script>

    <script src="~/Scripts/AdminPanelScripts/jquery.core.js"></script>
    <script src="~/Scripts/AdminPanelScripts/jquery.app.js"></script>




    @Scripts.Render("~/scripts/Json Scripts/registerJson.js")

    <script src="~/Scripts/Json Scripts/CreateJson.js"></script>

    @RenderSection("scripts", required: false)

    <!-- Scripts for images -->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/baguettebox.js/1.8.1/baguetteBox.min.js"></script>
    
</body>
</html>


